<script>
  $(document).ready(function(){
    $("#signupForm").submit(function(e) {

      var url = "/notifyUser"; // the script where you handle the form input.


        $.ajax({
          type: "GET",
          url: url,
          data:  {},
          success: function(data)
          {
            window.location.replace("/notify");


          }
        });
     });
  });


